ns1:T9 a ns1:Template ;
    ns1:explanation "" ;
    ns1:id "T9" ;
    ns1:text """{<literal 1>|{A|An} <term 1>
  {of {a|an} <term 2>| }}
is by definition
{<literal 2>|
[<literal 3>, or] from a <literal 4> to the following <literal 5>}.
""" .

ns1:S10 a ns1:Subtemplate ;
    ns1:explanation """A predicate is whatever may follow a subject in a clause.
""" ;
    ns1:id "S10" ;
    ns1:text """<predicate>::=
{{<verb phrase>| is {<equality operator>|<inequality operator>}}
  <object>|
<verb phrase> {[<object>, and] | [<object>, or] | }}
""" ;
    ns1:usesSubtemplate ns1:S11,
        ns1:S6,
        ns1:S7 .

