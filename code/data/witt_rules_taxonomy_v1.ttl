    ns1:id "T7" ;
    ns1:text """{A|An} <term 1>
  {of {a|an} <term 2>| }
is by definition
{a|an|the} <term 3>
  <qualifying clause>.
""" ;
    ns1:usesSubtemplate ns1:S14 .

ns1:T8 a ns1:Template ;
    ns1:explanation "" ;
    ns1:id "T8" ;
    ns1:text """{A|An} <term 1>
  {of {a|an} <term 2>| }
is by definition
