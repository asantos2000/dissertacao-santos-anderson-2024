    "\u00a7 275.0-7_P1|true_table": {
        "id": "\u00a7 275.0-7_P1",
        "type": "true_table",
        "content": {
            "section": "\u00a7 275.0-7",
            "summary": "This document defines what constitutes a 'small business' or 'small organization' under the Investment Advisers Act of 1940 for the purposes of the Regulatory Flexibility Act. It specifies criteria related to assets under management, total assets, and control relationships. It also provides definitions for 'control' and 'total assets' in the context of investment advisers.",
            "elements": [
                {
                    "id": 1,
                    "statement": "The term small business or small organization for purposes of the Investment Advisers Act of 1940 shall mean an investment adviser that has assets under management of less than $25 million.",
                    "terms": [
                        {
                            "term": "Small business",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Small organization",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Investment Advisers Act of 1940",
                            "classification": "Proper Noun"
                        },
                        {
                            "term": "Investment adviser",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Assets under management",
                            "classification": "Common Noun"
                        }
                    ],
                    "verb_symbols": [
                        "shall mean",
                        "has"
                    ],
                    "classification": "Fact Type",
                    "source": "(a)(1)"
                },
                {
                    "id": 2,
                    "statement": "The term small business or small organization for purposes of the Investment Advisers Act of 1940 shall mean an investment adviser that did not have total assets of $5 million or more on the last day of the most recent fiscal year.",
                    "terms": [
                        {
                            "term": "Small business",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Small organization",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Investment Advisers Act of 1940",
                            "classification": "Proper Noun"
                        },
                        {
                            "term": "Investment adviser",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Total assets",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Fiscal year",
                            "classification": "Common Noun"
                        }
                    ],
                    "verb_symbols": [
                        "shall mean",
                        "did not have"
                    ],
                    "classification": "Fact Type",
                    "source": "(a)(2)"
                },
                {
                    "id": 3,
                    "statement": "The term small business or small organization for purposes of the Investment Advisers Act of 1940 shall mean an investment adviser that does not control, is not controlled by, and is not under common control with another investment adviser that has assets under management of $25 million or more.",
                    "terms": [
                        {
                            "term": "Small business",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Small organization",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Investment Advisers Act of 1940",
                            "classification": "Proper Noun"
                        },
                        {
                            "term": "Investment adviser",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Control",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Assets under management",
                            "classification": "Common Noun"
                        }
                    ],
                    "verb_symbols": [
                        "shall mean",
                        "does not control",
                        "is not controlled by",
                        "is not under common control with"
                    ],
                    "classification": "Fact Type",
                    "source": "(a)(3)"
                },
                {
                    "id": 4,
                    "statement": "Control means the power, directly or indirectly, to direct the management or policies of a person, whether through ownership of securities, by contract, or otherwise.",
                    "terms": [
                        {
                            "term": "Control",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Power",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Management",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Policies",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Person",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Ownership of securities",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Contract",
                            "classification": "Common Noun"
                        }
                    ],
                    "verb_symbols": [
                        "means",
                        "to direct"
                    ],
                    "classification": "Fact Type",
                    "source": "(b)(1)"
                },
                {
                    "id": 5,
                    "statement": "A person is presumed to control a corporation if the person directly or indirectly has the right to vote 25 percent or more of a class of the corporation's voting securities.",
                    "terms": [
                        {
                            "term": "Person",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Corporation",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Right to vote",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Class of voting securities",
                            "classification": "Common Noun"
                        }
                    ],
                    "verb_symbols": [
                        "is presumed to control",
                        "has"
                    ],
                    "classification": "Fact Type",
                    "source": "(b)(1)(i)(A)"
                },
                {
                    "id": 6,
                    "statement": "A person is presumed to control a partnership if the person has the right to receive upon dissolution, or has contributed, 25 percent or more of the capital of the partnership.",
                    "terms": [
                        {
                            "term": "Person",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Partnership",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Right to receive upon dissolution",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Capital of the partnership",
                            "classification": "Common Noun"
                        }
                    ],
                    "verb_symbols": [
                        "is presumed to control",
                        "has"
                    ],
                    "classification": "Fact Type",
                    "source": "(b)(1)(ii)"
                },
                {
                    "id": 7,
                    "statement": "A person is presumed to control a limited liability company (LLC) if the person directly or indirectly has the right to vote 25 percent or more of a class of the interests of the LLC.",
                    "terms": [
                        {
                            "term": "Person",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Limited liability company",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Right to vote",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Class of the interests",
                            "classification": "Common Noun"
                        }
                    ],
                    "verb_symbols": [
                        "is presumed to control",
                        "has"
                    ],
                    "classification": "Fact Type",
                    "source": "(b)(1)(iii)(A)"
                },
                {
                    "id": 8,
                    "statement": "A person is presumed to control a trust if the person is a trustee or managing agent of the trust.",
                    "terms": [
                        {
                            "term": "Person",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Trust",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Trustee",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Managing agent",
