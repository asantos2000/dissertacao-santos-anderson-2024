                        }
                    ],
                    "verb_symbols": [
                        "is presumed to control",
                        "is"
                    ],
                    "classification": "Fact Type",
                    "source": "(b)(1)(iv)"
                },
                {
                    "id": 9,
                    "statement": "Total assets means the total assets as shown on the balance sheet of the investment adviser or other person described above under paragraph (a)(3) of this section.",
                    "terms": [
                        {
                            "term": "Total assets",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Balance sheet",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Investment adviser",
                            "classification": "Common Noun"
                        },
                        {
                            "term": "Person",
                            "classification": "Common Noun"
                        }
                    ],
                    "verb_symbols": [
                        "means",
                        "as shown on"
                    ],
                    "classification": "Fact Type",
                    "source": "(b)(2)"
                }
            ]
        }
    }
